{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "\u250f[<#ffffff>\ueb99</> {{ .UserName }}]",
          "foreground": "#7eb8da",
          "type": "session",
          "style": "plain"
        },
        {},
        {
          "properties": {
            "root_icon": "\uf292"
          },
          "template": "\uf0e7",
          "type": "root",
          "style": "diamond"
        },
        {
          "template": "[<#ffffff>\ue266</> RAM: {{ (div ((sub .PhysicalTotalMemory .PhysicalFreeMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }}GB]",
          "foreground": "#be9ddf",
          "type": "sysinfo",
          "style": "diamond"
        },
        {
          "properties": {
            "style": "folder"
          },
          "template": "[<#98bfad>\ue5ff {{ .Path }}</>]",
          "foreground": "#7eb8da",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": "[<#ffffff>{{ .UpstreamIcon }}</>{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} <#ffffff>\uf044</> {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} <#ffffff>\uf046</> {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} <#ffffff>\ueb4b</> {{ .StashCount }}{{ end }}]",
          "foreground": "#ffa5d8",
          "type": "git",
          "style": "plain"
        },
        {
          "template": "[{{ .Profile }}{{if .Region}}@{{ .Region }}{{ end }}]",
          "foreground": "#ffa5d8",
          "type": "aws",
          "style": "plain"
        },
        {
          "template": "[{{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}]",
          "foreground": "#ffa5d8",
          "type": "kubectl",
          "style": "plain"
        },
        {
          "template": "[\uf0e7]",
          "foreground": "#ffa5d8",
          "type": "root",
          "style": "plain"
        },
        {
          "template": "[<#ffffff>\uea6c</> Error, check your command]",
          "foreground": "#ffa5d8",
          "type": "status",
          "style": "powerline"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "#time_format": "Monday at 3:04:05 PM",
            "time_format": "15:04:05"
          },
          "template": "\u2516[<#ffffff>\uf108</> {{ .CurrentDate | date .Format }}]",
          "foreground": "#7eb8da",
          "type": "time",
          "style": "diamond"
        },
        {
          "template": " â™¥",
          "foreground": "#ffa5d8",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    },
    {}
  ],
  "version": 3
}
